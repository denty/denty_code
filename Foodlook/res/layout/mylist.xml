<Button android:text="提交订单" android:id="@+id/Button_send" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button><LinearLayout android:id="@+id/LinearLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content">
